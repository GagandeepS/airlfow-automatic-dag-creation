query	:	truncate {schema}.{table_name}; insert into {schema}.{table_name} ( with e as ( select *, convert_timezone( 'UTC', 'Australia/Sydney', collector_tstamp ) as best_tstamp from atomic.events where collector_tstamp >= convert_timezone('Australia/Sydney', 'UTC', '{from_date}') AND collector_tstamp < convert_timezone('Australia/Sydney', 'UTC', '{to_date}') AND platform = 'mob' AND app_id <> '5.0.8'), module_views as ( select * from e join atomic.au_net_abc_snowplow_collection_context_1 c on e.event_id = c.root_id and e.collector_tstamp = c.root_tstamp where e.event = 'struct' and e.se_category = 'collection' and e.se_action = 'view' ), conversions as ( select * from e join au_net_abc_snowplow_link_referrer_1 lr on e.event_id = lr.root_id and e.collector_tstamp = lr.root_tstamp where ( ( se_category in ('article', 'screen') and se_action = 'view' ) or (se_action = 'play') ) ), user_conversions as ( select date(m.best_tstamp) as batch_date, m.user_id as user_id, case when m.v_tracker like 'ios-%' then 'iOS' when m.v_tracker like 'andr-%' then 'android' else 'unknown' end as os, m.app_id as app_id, m.moduleid as moduleid, m.modulelabel as modulelabel, case when m.modulecontext like 'coremedia://article/%' then 'coremedia://article/%' when m.modulecontext like 'coremedia://video/%' then 'coremedia://video/%' when m.modulecontext like 'coremedia://audio/%' then 'coremedia://audio/%' when m.modulecontext like 'coremedia://recipe/%' then 'coremedia://recipe/%' when m.modulecontext like 'papi://audio%' then 'papi://audio%' when m.modulecontext like 'au.net.abc.ABC://Topic/%' then 'au.net.abc.ABC://Topic/%' when m.modulecontext like 'au.net.abc.ABC://topic/%' then 'au.net.abc.ABC://topic/%' else m.modulecontext end as modulecontext_group, m.se_category as se_category, m.se_action as se_action, count(distinct m.event_id) view_events, count(distinct c.event_id) conversion_events from module_views m left join conversions c on c.user_id = m.user_id and m.moduleid = c.moduleid and c.modulecontext = m.modulecontext and c.collector_tstamp between m.collector_tstamp and m.collector_tstamp + interval '30 min' group by 1, 2, 3, 4, 5, 6, 7, 8, 9 ) select * from user_conversions );
schema	:	modules
table_name	:	app_modules_daily_users_new
table_columns	:	batch_date,user_id,os,app_id,moduleid,modulelabel,modulecontext_group,se_category,se_action,view_events,conversion_events
start_date	:	2019-07-01
